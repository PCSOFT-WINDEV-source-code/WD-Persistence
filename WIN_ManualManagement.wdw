     code : |1+
      // Refresh the statics containing the variables
      STC_NB_LAUNCH = StringBuild(STC_NB_LAUNCH, gnNbLaunch)
      STC_VAR1 = StringBuild(<§$0000§>, gnNbLaunch)
      STC_VAR2 = StringBuild(<§$0001§>, gdDateLastLaunch)
      STC_VAR3 = StringBuild(<§$0002§>, gtTimeLastLaunch)
      
      // Refresh the static if the window has already been run
      IF gnNbLaunch >= 1 THEN
      	STC_LAST_LAUNCH = StringBuild(STC_LAST_LAUNCH, DateToString(gdDateLastLaunch, <§$0003§>), TimeToString(gtTimeLastLaunch, <§$0004§>))
      ELSE
      	STC_LAST_LAUNCH = <§$0005§>
      END
      
      
      // Increment the number of startups and save the date and time of the last startup
      gnNbLaunch++
      gdDateLastLaunch = DateSys()
      gtTimeLastLaunch = TimeSys()
