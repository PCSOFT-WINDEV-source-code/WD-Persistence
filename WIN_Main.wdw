     code : |1-
      // Summary: Opens the optimization window
      // Syntax:
      // _Open_Optimization ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      PROCEDURE _Open_Optimization()
      
      // Local variables
      i is int
      
      i = 1
      // The WinOptim window must be reopened as long as it returns True
      WHILE Open(WIN_Optim,i)
      	i++
      END
      
      // Display a message
      Info(<§$0007§>,<§$0008§>)
