     type_code : 14
     code : |1-
      // Summary: Opens the window for backup configuration
      // Syntax:
      // _Open_BackupConfiguration ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      PROCEDURE _Open_BackupConfiguration()
      
      // Open the configuration window
      Open(WIN_Config)
      
      // Initialize the backup of parameters
      InitParameter(pGlobal.gnBackupType,pGlobal.gsBackupPath)
      
      // Refresh the static for the backup mode
      REFBackupMode()
