      // --------------------------------------------
      // MANAGE THE EXAMPLE PARAMETERS
      // --------------------------------------------
      
      // Retrieve the configuration parameters of the example
      InitParameter(paramIni, CS_EXAMPLE_CONFIG)
      
      // Load the backup parameters
      pGlobal.gnBackupType = LoadParameter(CS_BACKUP_TYPE, -1)
      pGlobal.gsBackupPath = LoadParameter(CS_BACKUP_PATH)
      
      IF InAutomaticTestMode() THEN
      	pGlobal.gnBackupType = paramIni
      	pGlobal.gsBackupPath = CS_INI_FILE
      END
      
      // If no type of backup
      IF pGlobal.gnBackupType = -1 THEN
      	
      	Info(<§$0000§>)
      	
      	// The example is started for the first time, we must specify the configuration parameters of the example
      	Open(WIN_Config, True)	
      	
      	// Parameters saved
      	IF pGlobal.gnBackupType = -1 THEN
      		Error(<§$0001§>)
      		EndProgram()
      	END
      	
      END
      
      
      // ---------------------------------------------
      // Initialize the backup parameters
      // ---------------------------------------------
      
      // Initialize the last parameters
      InitParameter(pGlobal.gnBackupType, pGlobal.gsBackupPath)	
