  procedures :
   -
     name : REFBackupMode
     internal_properties : CAAAAAgAAABm51fBj1rE3U4O/1hnvYNe2FWqtTrHwUiFrxqUCHXg7+YPdakvu1k4IXJLufC09tFE7zGQGgyzNqHZ9ridHJseu/mhlvZsUQCJwniz8cwxtqXq6cAznnEyAGovqJGkUYiVWni70CXI89SQQlm9
     procedure_id : 1624923103194429121
     type_code : 14
     code : |1-
      // Syntax:
      //REFBackupMode ()
      //
      // This procedure is used to refresh the static containing the current backup mode
      
      PROCEDURE REFBackupMode()
      
      sBackupMode is string = <§$0002§>
      
      // According to the type of backup currently defined
      // Compare it to the WLanguage constants used with InitParameter
      SWITCH pGlobal.gnBackupType
      
      	CASE paramRegistry // Registry
      		sBackupMode = StringBuild(sBackupMode,<§$0003§>)
      		
      	CASE paramXML // XML file
      		sBackupMode = StringBuild(sBackupMode,<§$0004§>)
      		
      	CASE paramIni // INI file
      		sBackupMode = StringBuild(sBackupMode,<§$0005§>)
      		
      	OTHER CASE 
      		// Unmanaged
      		
      END
      // Check the backup path
      IF pGlobal.gsBackupPath = "" THEN
      	sBackupMode = StringBuild(sBackupMode,<§$0006§>)
      ELSE
      	sBackupMode = StringBuild(sBackupMode, pGlobal.gsBackupPath)	
      END
      
      
      STC_BACKUP_MODE = sBackupMode
     type : 458752
   -
     name : _Open_BackupConfiguration
