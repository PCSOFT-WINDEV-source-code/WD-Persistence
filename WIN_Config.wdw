      PROCEDURE WIN_Config(LOCAL gbInit is boolean = False)
      
      // The backup parameters must be saved in the configuration file of the example
      // This is the reason why the configuration of the parameters is reset with the Config.ini file of the example
      InitParameter(paramIni, CS_EXAMPLE_CONFIG)
   -
     code : |1-
      
      // If it is the first startup, the button for re-initializing the backups is useless
      // Hide it
      IF gbInit THEN
      	BTN_RESET..Visible = False
      END
      // Change the description, if it is not the first launch, there is a saved value that will be restored
      // The modification code will be run during the restore operation (see the optimization window)
      ChangeDescription()
