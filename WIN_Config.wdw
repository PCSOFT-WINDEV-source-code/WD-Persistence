  procedures :
   -
     name : ChangeDescription
     procedure_id : 1624923094603706675
     type_code : 14
     code : |1+
      // Syntax:
      //ChangeDescription ()
      //
      // This procedure is used to update the description according to the backup setting
      // 
      PROCEDURE ChangeDescription()
      
      sMessage is string // The message that will be displayed in the description control
      bRegistryKey is boolean // Is the backup saved in a registry key?
      sParameter4 is string // The parameter 4 corresponds to the location where the parameters are saved (registry key or file name)
      sPersonalName is string // Personal name of the file
      
      sMessage = <§$0000§>
      
      // -----------------------------------------------------
      // Build the first part of the description
      // -----------------------------------------------------
      
      // According to the type of backup
      SWITCH RADIO_TYPE_SAVE
      
      	CASE 1 // Registry
      		
      		// We are in a registry key
      		bRegistryKey = True
      		
      		// Build the first part of the description
      		sMessage = StringBuild(sMessage, <§$0001§>, <§$0002§>, <§$0003§>, <§$0004§>)
      		
      		
      	CASE 2 // XML file
      		
      		// Build the first part of the description
      		sMessage = StringBuild(sMessage, <§$0005§>, <§$0006§>, <§$0007§>, <§$0004§>)
      		
      		// Save the name of the file in case of manual management
      		sPersonalName = CS_XML_FILE
      		
      	CASE 3 // INI file
      		
      		// Build the first part of the description
      		sMessage = StringBuild(sMessage, <§$0008§>, <§$0009§>, <§$0007§>, <§$0004§>)
      		
      		// Save the name of the file in case of manual management
      		sPersonalName = CS_INI_FILE
      		
      	OTHER CASE 
      		// The case is not managed
      		sMessage = ""
      		
      END
      
      // -----------------------------------------------------
      // Build the second part of the description
      // -----------------------------------------------------
      
      // According to the management mode of parameter backup
      SWITCH RADIO_MANAGEMENT_SAVE
      	CASE 1 // Automatic management of parameters
      		
      		// Are we in the registry
      		IF bRegistryKey = True THEN
      			// Retrieve the key corresponding to the project
      			sParameter4 = ProjectInfo(piRegistry)
      			
      		ELSE
      			// The name of the backup file is the name of the project
      			sParameter4 = fExeDir() + <§$000a§> + fExtractPath(sPersonalName, fExtension)
      			
      		END
      		
      		// Build the second part of the description
      		sMessage = StringBuild(sMessage, <§$000b§>, sParameter4)
      		sMessage += CR+ CR + <§$000c§>
      		
      		
      	CASE 2 // Manual management of parameters (the backup paths are specified manually)
      		
      		IF bRegistryKey = True THEN
      			// we are in the registry so the parameters are saved in the personal key
      			sParameter4 = CS_REGISTRY
      		ELSE
      			// We are in a file
      			sParameter4 = sPersonalName		
      			
      		END
      		
      		
      		// Build the second part of the description
      		sMessage = StringBuild(sMessage, <§$000d§>, sParameter4)
      		sMessage += CR +CR + <§$000e§>
      		
      	OTHER CASE
      		// The case is not managed
      		sMessage = ""
      END
      
      
      // update the static
      STC_DESCRIPTION = sMessage
     type : 458752
   -
     name : SaveParameters
     internal_properties : CAAAAAgAAAAGR7dh7zpcTR7ur7g3jdOuqFVa5bpXoWhFf5pkyHXgz+b/lUk/+6koMVIbmQCkBkHUb6Ggenz7bvnhnmA1dFNGQ5E5ji7UuTghKgD7SWSpPi1iYXiLRhkKONKX0ClcCdBdsjBzKI0oU3TwIrnd
     procedure_id : 1624923094603772211
     type_code : 14
     code : |1-
      // Syntax:
      //SaveParameters ()
      //
      // This function is used to save the current parameters according to the type of backup
      
      PROCEDURE SaveParameters()
      
      sBackupPath is string // Path of backup
      nBackupType is int // Type of backup
      
      
      // Initialize the backup mode
      SWITCH RADIO_TYPE_SAVE
      
      	CASE 1 // Registry		
      		nBackupType = paramRegistry   
      		sBackupPath = CS_REGISTRY
      		
      	CASE 2 // XML file	
      		nBackupType = paramXML	
      		sBackupPath = CS_XML_FILE
      		
      	CASE 3 // INI file	
      		nBackupType = paramIni
      		sBackupPath = CS_INI_FILE
      		
      	OTHER CASE 
      		// Unmanaged
      		RETURN
      END
      
      
      // Automatic or manual backup
      SWITCH RADIO_MANAGEMENT_SAVE
      
      	CASE 1 // Automatic
      		sBackupPath = ""
      		
      	CASE 2 // Manual
      		// Don't change
      		
      END
      
      // Save the parameters 
      pGlobal.gnBackupType = nBackupType
      pGlobal.gsBackupPath = sBackupPath
      
      Close()
     type : 458752
